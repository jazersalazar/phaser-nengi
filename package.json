{
  "name": "phaser-nengi",
  "version": "0.0.0",
  "description": "PhaserJS with Nengi demo project",
  "main": "index.js",
  "scripts": {
    "start": "npx npm-run-all --parallel server client",
    "server": "npx nodemon ./server/index.js --ignore public",
    "client": "npx webpack-dev-server --config webpack.dev.js",
    "build": "npx webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazersalazar/phaser-nengi.git"
  },
  "keywords": [
    "PhaserJS",
    "Nengi"
  ],
  "author": "Jazer Salazar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jazersalazar/phaser-nengi/issues"
  },
  "homepage": "https://github.com/jazersalazar/phaser-nengi#readme",
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "terser-webpack-plugin": "^5.1.4"
  }
